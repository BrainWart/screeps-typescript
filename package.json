{
  "name": "screeps-typescript",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json \"src/**/*.ts\"",
    "build": "rollup -c",
    "console": "multimeter.cmd",
    "push-main": "rollup -c --environment DEST:main",
    "push-seasonal": "rollup -c --environment DEST:seasonal",
    "push-sim": "rollup -c --environment DEST:sim",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "rollup -c rollup.test-unit-config.js && mocha dist/test-unit.bundle.js",
    "test-integration": "npm run build && rollup -c rollup.test-integration-config.js && mocha dist/test-integration.bundle.js",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-sim": "rollup -cw --environment DEST:sim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrainWart/screeps-typescript.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/BrainWart/screeps-typescript/issues"
  },
  "homepage": "https://github.com/BrainWart/screeps-typescript",
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/chai": "^4.1.6",
    "@types/lodash": "3.10.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.13.1",
    "@types/screeps": "^3.2.3",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@typescript-eslint/typescript-estree": "^4.29.1",
    "chai": "^4.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lodash": "^4.17.21",
    "mocha": "^10.4.0",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-git-version": "^0.3.1",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.31.0",
    "screeps-multimeter": "^2.1.1",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "ts-node": "^10.2.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@brainwart/screeps-prometheus-game": "^1.0.1",
    "source-map": "~0.6.1"
  }
}
